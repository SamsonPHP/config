{
	"name":"samsonphp/config",
    "description": "SamsonPHP configuration system",
    "keywords": ["file system", "samsonphp"],
    "authors":[
        {
            "name":"Vitaly Iegorov",
            "email":"egorov@samsonos.com"
        }
    ],
    "require": {
        "samsonphp/event": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
	"license":"Open Software License (OSL) v 3.0",
	"homepage":"http://samsonos.com/",
    "autoload" : {
        "classmap": ["src/"],
        "psr-0": {
            "samsonos\\config\\": "src/"
        },
        "files" : ["src/samson.php"]
    },
    "extra": { "samson_module_ignore": "1" }
}
